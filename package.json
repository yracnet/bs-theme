{
  "name": "bootstrap-npm-starter",
  "version": "1.4.0",
  "private": true,
  "description": "Demo project for building a Bootstrap powered site via npm.",
  "author": "mdo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twbs/bootstrap-npm-starter.git"
  },
  "bugs": {
    "url": "https://github.com/twbs/bootstrap-npm-starter/issues"
  },
  "homepage": "https://github.com/twbs/bootstrap-npm-starter#readme",
  "scripts": {
    "build": "npm run css",
    "css-compile-min": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o assets/css/",
    "css-compile": "node-sass --include-path node_modules --source-map true --source-map-contents true --precision 6 scss -o assets/css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace assets/css/bootstrap.css --map",
    "css": "npm-run-all css-compile css-prefix",
    "server": "serve --listen 3003",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm run css-lint && npm run css"
  },
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "javascript",
    "starter",
    "template"
  ],
  "dependencies": {
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-rename": "^0.6.0",
    "purgecss": "^4.0.3",
    "serve": "^12.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-twbs-bootstrap": "^2.2.3"
  }
}